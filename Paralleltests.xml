<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="4">
	<test name="Exam center">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="ExamCenterRunners.Location_Runner"></class>
			<class name="ExamCenterRunners.Users_Creation_Runner"></class>
			<class name="ExamCenterRunners.Exam_Schedule_runner"></class>
			<class name="ExamCenterRunners.Create_Time_Slot_and_Enroll_the_ExamTakerRunner"></class>
			<class name="ExamCenterRunners.AssessmentSubmissionRunner"></class>
			<class name="ExamCenterRunners.Enrolled_ExamTaker_in_a_TimeSlotRunner"></class>
			<class name="ExamCenterRunners.Score_SubmissionRunner"></class>
			<class name="ExamCenterRunners.EnrolleRunner"></class>
		</classes>
	</test>

	<test name="Assessment center">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="AssessmentsRunner.BenchmarksRunner" />
			<class name="AssessmentsRunner.QuizCreationRunner" />
			<class name="AssessmentsRunner.AnnouncementsRunner" />
			<class name="AssessmentsRunner.QuizSubmissionRunner" />
			<class name="AssessmentsRunner.PerformingExamRunner" />
			<class name="AssessmentsRunner.ScoreSubmissionRunner" />
			<class name="AssessmentsRunner.Teacher_ActivityprogressRunner" />
			<class name="AssessmentsRunner.LaunchValidationRunner" />
			<class name="AssessmentsRunner.Reportcard_TeacherRunner" />
		</classes>
	</test>

	<test name="Portfolio center">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="PortfolioCenterRunner.PortfolioCenterCourseRunner"></class>
			<class name="PortfolioCenterRunner.AssignmentsSubmissionRunner"></class>
			<class name="PortfolioCenterRunner.PortfolioScoreSubmissionRunner"></class>
		    <class name="PortfolioCenterRunner.MultiScoringPortfolioCourseRuner"></class>
			<class name="PortfolioCenterRunner.MultiScoringAssignmentSubmissionRunner"></class>
			<class name="PortfolioCenterRunner.MultiScoringSubmissionRuner"></class>
			<class name="PortfolioCenterRunner.DeletePortfolioCourseRunner"></class>
		</classes>
	</test>
	
	<test name="Learning Tree">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="LearningTreeRunner.Activity_CreationRunner"/>
			<class name="LearningTreeRunner.Import_Activities_in_LTRunner"/>
			<class name="LearningTreeRunner.Student_Activities_SubmissionRunner"/>
			<class name="LearningTreeRunner.Provide_Score_in_LTRunner"/>	
			<class name="LearningTreeRunner.DocumentsRunner"/>			
			<class name="LearningTreeRunner.ReportcardRunner"/>	
		</classes>
	</test>
</suite> <!--
Suite -->